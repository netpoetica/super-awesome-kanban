{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-classes-per-file": 0,
        "no-undef": 0,
        "react/prop-types": 0,
        "react/jsx-filename-extension": 0,
        "react/react-in-jsx-scope": 0,
        "react/prefer-stateless-function": 0
    }
}